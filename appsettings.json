{
  "ConnectionStrings": {
    "Default": "Data Source=beershop.db"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "EGAIS": {
    "ApiUrl": "https://egais-api.example.com/v1",
    "Login": "your_login",
    "Password": "your_password"
  },
  "Fiscal": {
    "ComPort": "COM3",
    "DriverType": "Atol"
  },
  "BarcodeScanner": {
    "ComPort": "COM4",
    "BaudRate": 9600
  },
  "PaymentTerminal": {
    "ComPort": "COM5",
    "Provider": "Ingenico"
  }
}